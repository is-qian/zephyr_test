/ {
	chosen {
		zephyr,console = &uart21;
		zephyr,shell-uart = &uart21;
		nordic,rpc-uart = &uart21;
	};

	zephyr,user {
		io-channels = <&adc 0>, <&adc 1>, <&adc 2>, <&adc 3>,
			<&adc 4>, <&adc 5>, <&adc 6>, <&adc 7>;
	};
};

&uart21 {
 	current-speed = <921600>;
};
&uart20 {
	status = "disabled";
};